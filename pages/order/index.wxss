page {
  background: #f5f5f5;
}
.order_item {
  margin-top: 10rpx;
}
.order_time {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #555555;
  margin-left: 26rpx;
}
.order_info {
  width: 710rpx;
  height: 140rpx;
  padding: 20rpx 10rpx 0rpx 20rpx;
  background: #ffffff;
  margin: auto;
}
.order_info .order_gas_name {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: bold;
  color: #222222;
}
.order_info .order_gas_name image {
  width: 32rpx;
  height: 32rpx;
}
.order_info .order_details {
  display: flex;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  line-height: 48px;
  color: #888888;
}
.order_info .order_details .order_oil {
  margin-right: 104rpx;
}
.order_info .order_details .order_detail_price {
  margin-left: auto;
  margin-right: 20rpx;
}
.no_order {
  width: 100vw;
  text-align: center;
  font-size: 32rpx;
  font-weight: 600;
  margin-top: 400rpx;
}
