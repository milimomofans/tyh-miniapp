page {
  background: #f5f5f5;
}
.prompt {
  width: 100vw;
  height: 40rpx;
  line-height: 40rpx;
  background: rgba(255, 51, 51, 0.1);
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #ff3333;
  text-align: center;
}
.gasOrder_item {
  width: 710rpx;
  height: 380rpx;
  margin: 10rpx auto;
  box-sizing: border-box;
  padding: 20rpx 10rpx 20rpx 40rpx;
  background: white;
}
.item_head {
  width: 100%;
  display: flex;
  justify-content: space-between;
  font-size: 32rpx;
  font-weight: bold;
  color: #222222;
  font-family: PingFang SC;
  align-items: center;
  margin-bottom: 16rpx;
}
.item_head image {
  width: 32rpx;
  height: 32rpx;
}
.item_attr {
  display: flex;
  padding: 4rpx;
}
.item_attr .item_attr_left {
  width: 140rpx;
  margin-right: 20rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #222222;
}
.item_attr .item_attr_right {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #888888;
}
.section {
  display: flex;
  align-items: center;
}
.section .nowTime {
  margin-left: 20rpx;
  margin-right: 14rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #555555;
}
.section .section_text {
  margin: 0 16rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #555555;
}
.section .picker {
  width: 140rpx;
  height: 60rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #222222;
  padding-left: 20rpx;
  line-height: 60rpx;
  background: white;
}
.section .serach {
  width: 160rpx;
  height: 60rpx;
  background: linear-gradient(135deg, #ff973d 0%, #ff3333 100%);
  border-radius: 8rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  margin: 0 5rpx;
  color: #ffffff;
}
.noOrder_text {
  height: 400rpx;
  text-align: center;
  line-height: 400rpx;
  font-size: 34rpx;
}
.total {
  width: 710rpx;
  padding: 20rpx 0;
  margin: 20rpx auto;
  text-align: center;
  font-size: 29rpx;
  font-weight: 500;
  background: white;
}
